      console.warn('âš ï¸ DriveApp ê²€ìƒ‰ ì‹¤íŒ¨, Drive APIë¡œ ì¬ì‹œë„:', driveAppError.message);
    }

    // ë°©ë²• 2: Drive API ì‚¬ìš© (fallback, 429 ì—ëŸ¬ ì²˜ë¦¬ í¬í•¨)
    try {
      const query = `name='${sheetName.replace(/'/g, "\\'")}' and mimeType='application/vnd.google-apps.spreadsheet' and trashed=false`;
      console.log('ğŸ“Š Drive API ê²€ìƒ‰ ì¿¼ë¦¬:', query);

      let retries = 3;
      let waitTime = 1000; // 1ì´ˆë¶€í„° ì‹œì‘
      
      while (retries > 0) {
        try {
          const files = Drive.Files.list({
            q: query,
            fields: 'files(id,name)'
