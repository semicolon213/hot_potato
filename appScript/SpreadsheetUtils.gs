          if (files.files && files.files.length > 0) {
            const spreadsheetId = files.files[0].id;
            console.log('âœ… Drive APIë¡œ ìŠ¤í”„ë ˆë“œì‹œíŠ¸ ID ì°¾ê¸° ì„±ê³µ:', spreadsheetId);
            return spreadsheetId;
          } else {
            console.warn('ğŸ“Š ìŠ¤í”„ë ˆë“œì‹œíŠ¸ë¥¼ ì°¾ì„ ìˆ˜ ì—†ìŠµë‹ˆë‹¤:', sheetName);
            return null;
          }
        } catch (apiError) {
          // 429 ì—ëŸ¬ì¸ ê²½ìš° ì¬ì‹œë„
          if (apiError.toString().includes('429') || apiError.toString().includes('rate limit')) {
            retries--;
            if (retries > 0) {
              console.warn(`âš ï¸ API í˜¸ì¶œ ì œí•œ (429). ${waitTime}ms í›„ ì¬ì‹œë„... (ë‚¨ì€ ì‹œë„: ${retries})`);
              Utilities.sleep(waitTime);
              waitTime *= 2; // ì§€ìˆ˜ ë°±ì˜¤í”„
            } else {
              console.error('âŒ Drive API ì¬ì‹œë„ ì‹¤íŒ¨:', apiError);
            }
          } else {
            // 429ê°€ ì•„ë‹Œ ë‹¤ë¥¸ ì—ëŸ¬ëŠ” ì¦‰ì‹œ throw
            throw apiError;
          }
        }
      }
    } catch (driveApiError) {
      console.error('âŒ Drive API ê²€ìƒ‰ ì‹¤íŒ¨:', driveApiError.message);
    }

    console.warn('ğŸ“Š ëª¨ë“  ë°©ë²•ìœ¼ë¡œ ìŠ¤í”„ë ˆë“œì‹œíŠ¸ë¥¼ ì°¾ì„ ìˆ˜ ì—†ìŠµë‹ˆë‹¤:', sheetName);
    return null;

  } catch (error) {
    console.error('ğŸ“Š ìŠ¤í”„ë ˆë“œì‹œíŠ¸ ID ì°¾ê¸° ì˜¤ë¥˜:', error);
    return null;
