function getSheetIdByName(sheetName) {
  try {
    console.log('ğŸ“Š ìŠ¤í”„ë ˆë“œì‹œíŠ¸ ID ì°¾ê¸° ì‹œì‘:', sheetName);

    // ë°©ë²• 1: DriveApp ì‚¬ìš© (ë‚´ì¥ ì„œë¹„ìŠ¤, 429 ì—ëŸ¬ ì—†ìŒ)
    try {
      const escapedName = sheetName.replace(/'/g, "\\'");
      const query = `title = '${escapedName}' and mimeType = 'application/vnd.google-apps.spreadsheet' and trashed = false`;
      console.log('ğŸ“Š DriveApp ê²€ìƒ‰ ì¿¼ë¦¬:', query);
      
      const files = DriveApp.searchFiles(query);
      const fileIterator = files;
      
      if (fileIterator.hasNext()) {
        const file = fileIterator.next();
        const spreadsheetId = file.getId();
        console.log('âœ… DriveAppìœ¼ë¡œ ìŠ¤í”„ë ˆë“œì‹œíŠ¸ ID ì°¾ê¸° ì„±ê³µ:', spreadsheetId);
        return spreadsheetId;
